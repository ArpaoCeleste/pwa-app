{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm start",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    
    {
      "src": "/api/(.*)",
      "dest": "https://pwa-app-lbb8.onrender.com/api/$1"
    },
  
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1"
    }
  ]
}